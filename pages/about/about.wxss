page {
    width: 100%;
    height: 100%;
    background: #000;
    position: relative;
}

view {
    width: 100%;
}
.scroll {
    height: 1100rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.weapp-about {
    width: 100%;
    height: 100%;
}

.weapp-name {
    font-size: 80rpx;
    text-align: center;
    color: #f5f5f5;
    margin-top: 400rpx;
}

.webapp-intro {
    box-sizing: border-box;
}

.webapp-intro-item {
    margin-bottom: 40rpx;
    padding: 0 40rpx;
}

.webapp-intro-title {
    margin-bottom: 10rpx;
    font-size: 50rpx;
    color: #666;
}

.webapp-intro-content {
    color: #ccc;
    font-size: 36rpx;
}
.over {
    margin-top: 900rpx;
    font-size: 80rpx;
    font-weight: 1400rpx;
    text-align: center;
    color: #f5f5f5;
    padding-bottom: 500rpx;
    text-shadow: 0 0 20px #fff;
}